package com.example.demo;

class Series<T>//Generic Class
{
	int a;
	int b;
	String str;
	float f;
	T obj;
	Series(T obj)
	{
		this.obj=obj;
	}
	public T getObj()
	{
		return obj;
	}
}
public class GenericsDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Series<Integer> s=new Series<Integer>(10);
		Series<String> s1=new Series<String>("Home of Programming");
		System.out.println("Integer Value: "+s.getObj());
		System.out.println("String value: "+s1.getObj());
	}

}
