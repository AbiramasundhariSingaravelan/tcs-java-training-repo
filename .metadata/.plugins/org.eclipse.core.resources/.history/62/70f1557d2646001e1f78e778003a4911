import java.util.ArrayList;
import java.util.List;

class Product
{
	int id;
	String name;
	float price;
	
	public Product() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Product(int id, String name, float price) {
		super();
		this.id = id;
		this.name = name;
		this.price = price;
	}
	
}
