package com.example.demo;

class Series<T>//Generic Class
{
	T obj;
	Series(T obj)
	{
		this.obj=obj;
	}
	public T getObj()
	{
		return obj;
	}
}
public class GenericsDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Series<Integer> s=new Series<Integer>(10);
	}

}
